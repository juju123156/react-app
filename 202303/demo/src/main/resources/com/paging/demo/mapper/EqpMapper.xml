<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.paging.demo.mapper.EqpMapper">
    <select id="findOrg" resultType="EqpDto">
        <![CDATA[
            SELECT *
            FROM EQP_INF
            ORDER BY EQP_ID
            LIMIT #{pageStart}, #{perPageNum}
        ]]>
    </select>


    <select id="eqpListCnt" resultType="int">
        SELECT  count(*)
        FROM EQP_INF
    </select>
</mapper>